    "description": "Tala Payments is a framework independent, multi-gateway payment processing library",
    "keywords": ["payment", "payments", "merchant", "paypal", "gateway"],
    "homepage": "https://github.com/adrianmacneil/tala-payments",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        }
    ],
    "autoload": {
        "psr-0": { "Tala\\Core" : "src/" }
    },
    "require": {
        "kriswallsmith/buzz": "0.7",
        "symfony/http-foundation": "2.*"
    },
    "require-dev": {
