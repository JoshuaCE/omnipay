{
    "name": "adrianmacneil/tala-payments",
    "type": "library",
    "description": "Tala Payments is a framework agnostic multi-gateway payment processing library",
    "keywords": ["payment", "merchant", "gateway", "paypal", "authorize.net", "dps", "paymentexpress"],
    "homepage": "https://github.com/adrianmacneil/tala-payments",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        }
    ],
    "autoload": {
        "psr-0": { "Tala" : "src/" }
    },
    "require": {
        "symfony/http-foundation": "2.*"
    },
    "require-dev": {
        "kriswallsmith/buzz": "0.*",
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.7.*",
        "squizlabs/php_codesniffer": "1.4.*"
    },
    "extra": {
        "branch-alias": { "dev-master": "0.1-dev" }
    }
}
